{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": true,
		"checkJs": true,
		"module": "node16",
		"moduleResolution": "node16",
		"lib": ["es2023"],
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"baseUrl": "./",
		"forceConsistentCasingInFileNames": true,
		"allowImportingTsExtensions": true,
		"paths": {
			"#common": ["config/common.js"]
		}
	}
}
